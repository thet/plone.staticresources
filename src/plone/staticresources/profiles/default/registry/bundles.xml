<?xml version="1.0"?>
<registry>

  <records prefix="plone.bundles/bootstrap-js" interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="enabled">True</value>
    <value key="jscompilation">++plone++static/bundle-bootstrap/js/bootstrap.bundle.min.js</value>
    <value key="csscompilation"></value>
    <value key="last_compilation"></value>
    <value key="load_async">False</value>
    <value key="load_defer">False</value>
    <value key="compile">False</value>
  </records>

  <records prefix="plone.bundles/plone" interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="enabled">True</value>
    <value key="jscompilation">++plone++static/bundle-plone/bundle.min.js</value>
    <value key="csscompilation"></value>
    <value key="last_compilation"></value>
    <value key="load_async">False</value>
    <value key="load_defer">False</value>
    <value key="compile">False</value>
  </records>

  <record name="plone.resources.last_legacy_import">
    <field type="plone.registry.field.Datetime">
      <title>Last legacy js/css import date</title>
      <description>It's used to decide if its needed to compile or not the production css/js</description>
    </field>
    <value>2021-01-01 01:01:01</value>
  </record>

</registry>
